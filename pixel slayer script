-- one of my throwaway scripts dont take this seriously

local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")();
local win = DiscordLib:Window("\tDiscord");
local serv = win:Server("made by banovion", "http://www.roblox.com/asset/?id=13748779708");
local btns = serv:Channel("Buttons");

btns:Button("autofarm\nuse in priv server\ntoo lazy to make it a toggle so rejointo turn it off", function()
local hrp = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart;
local RunService: RunService = game:GetService("RunService");
local platform = Instance.new("Part");
platform.CanCollide = true;
platform.Anchored = true;
platform.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart;
platform.Size = Vector3.new(4, 1, 3);
platform.Name = "platform";

RunService.RenderStepped:Connect(function()
    platform.CFrame = hrp.CFrame * CFrame.new(0, -3.1, 0);
end)

--===========================================

local Clip = false;
local player = game.Players.LocalPlayer;

local function NoclipLoop()
    if not Clip and player.Character then
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                if part ~= platform then
                    part.CanCollide = false;
                end
            end
        end
    end
end

game:GetService("RunService").Stepped:Connect(NoclipLoop);

--===================================================================
local player = game:GetService("Players").LocalPlayer;
local character = player.Character or player.CharacterAdded:Wait();
local hrp = character:WaitForChild("HumanoidRootPart");
local target = workspace:WaitForChild("World"):WaitForChild("Mobs"):WaitForChild("Entity"):WaitForChild("HumanoidRootPart");

local newPosition = target.Position - Vector3.new(0, 4.8, 0);


while task.wait(0.01) do
hrp.CFrame = CFrame.new(newPosition);
hrp.CFrame *= CFrame.Angles(0, math.rad(90), 0);
end
end)

btns:Seperator();

local tgls = serv:Channel("Toggles");

local RunService: RunService = game:GetService("RunService");
tgls:Toggle("damage poison (basically poisons enemies when u hit\nthem as long as you stay in close range of them)", false, function(bool)
        while task.wait(1/20) do
        local args = {[1] = workspace:WaitForChild("World"):WaitForChild("Mobs"):WaitForChild("Entity")};
        game:GetService("ReplicatedStorage"):WaitForChild("Universe"):WaitForChild("Network"):WaitForChild("Remotes"):WaitForChild("DamageFire"):FireServer(unpack(args));
    end
end)

tgls:Seperator()

local running: Boolean = false;
tgls:Toggle("auto sword swing", false, function(bool)
    running = bool;

        if bool then
        local mouse = game:GetService("Players").LocalPlayer:GetMouse();
        local connections = getconnections(mouse.Button1Up);
    
    while running do
    for thxshock, connection in pairs(connections) do
    connection.Function();
    end

task.wait(1/100);

end
end
end)

tgls:Seperator()

local sldrs = serv:Channel("Sliders")

local sldr = sldrs:Slider("walkspeed slider", 0, 100, 16, function(t)
getgenv().Enabled = true;
getgenv().Speed = tonumber(t);
loadstring(game:HttpGet("https://raw.githubusercontent.com/eclipsology/SimpleSpeed/main/SimpleSpeed.lua"))()
end)
